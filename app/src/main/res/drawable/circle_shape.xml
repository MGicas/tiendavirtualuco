<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">

    <!-- Color de fondo del círculo -->
    <solid android:color="#468966" />  <!-- Puedes cambiar este color al que desees -->

    <!-- Bordes del círculo -->
    <stroke
        android:width="2dp"
        android:color="#468966" />  <!-- Cambia este color para el borde -->

    <!-- Tamaño del radio del círculo -->
    <size
        android:width="100dp"
        android:height="100dp" /> <!-- Ajusta el tamaño del círculo aquí si es necesario -->

    <!-- Radio del borde del círculo -->
    <corners android:radius="50dp" />  <!-- Radio grande para asegurar que sea completamente redondo -->

</shape>
